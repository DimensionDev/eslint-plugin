{
  "name": "@dimensiondev/eslint-plugin",
  "version": "0.0.2",
  "description": "eslint plugin for dimensiondev",
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "homepage": "https://dimensiondev.github.io/eslint-plugin",
  "repository": "https://github.com/DimensionDev/eslint-plugin",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "generate": "ts-node ./scripts/generate",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "publish-to-github": "./scripts/publish-to-github.sh",
    "test": "mocha"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "src/**/*": [
      "sh -c 'npm test'",
      "ts-node ./scripts/generate",
      "git add ."
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@types/prettier": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@typescript-eslint/types": "^5.10.0",
    "@typescript-eslint/utils": "^5.10.0",
    "commitlint": "^16.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^4.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "json-schema-to-typescript": "^10.1.5",
    "lint-staged": "^12.2.0",
    "mocha": "^9.1.4",
    "prettier": "^2.5.1",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "eslint": ">= 8"
  },
  "engines": {
    "node": ">= 16"
  }
}
