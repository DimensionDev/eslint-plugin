{
  "name": "@dimensiondev/eslint-plugin",
  "version": "0.0.2",
  "description": "eslint plugin for dimensiondev",
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "homepage": "https://dimensiondev.github.io/eslint-plugin",
  "repository": "https://github.com/DimensionDev/eslint-plugin",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "generate": "ts-node ./scripts/generate",
    "lint": "eslint . --fix",
    "markdownlint": "markdownlint-cli .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "publish-to-github": "./scripts/publish-to-github.sh",
    "spellcheck": "cspell lint --relative --no-progress '**'",
    "test": "mocha"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown",
      "cspell"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@types/glob": "^7.2.0",
    "@types/prettier": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@typescript-eslint/types": "^5.21.0",
    "@typescript-eslint/utils": "^5.21.0",
    "commitlint": "^16.2.4",
    "cspell": "^5.19.7",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "glob": "^8.0.1",
    "husky": "^7.0.4",
    "json-schema-to-typescript": "^10.1.5",
    "lint-staged": "^12.4.1",
    "markdownlint-cli": "^0.31.1",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "eslint": ">= 8"
  },
  "engines": {
    "node": ">= 16"
  }
}
